  },
  "homepage": "https://github.com/MrCreaper/drg-modding-compiler#readme",
  "dependencies": {
    "find-process": "^1.4.7",
    "fs-extra": "^10.1.0",
    "ini": "^3.0.1",
    "zip-lib": "^0.7.3"
  },
  "devDependencies": {
    "@angablue/exe": "^1.1.6",
    "pkg": "^5.8.0"
